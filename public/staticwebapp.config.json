{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/_next/*", "/favicon.ico", "/*.css", "/*.js"]
  },
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "mimeTypes": {
    ".css": "text/css",
    ".js": "application/javascript",
    ".json": "application/json"
  }
}
